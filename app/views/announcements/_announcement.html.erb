<div id="<%= dom_id announcement %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= announcement.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Announcement type:</strong>
    <%= announcement.announcement_type %>
  </div>
